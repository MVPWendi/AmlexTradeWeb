@{
    Layout = "_Layout";
}
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@using AmlexTradeWeb.Models

@model List<Plugin>
<div class="jumbotron">
    @foreach(Plugin plugin in Model)
    {
        <a asp-controller="Wiki" asp-action="Plugin" asp-route-Name="@plugin.Name">@plugin.Name</a>
    }
</div>